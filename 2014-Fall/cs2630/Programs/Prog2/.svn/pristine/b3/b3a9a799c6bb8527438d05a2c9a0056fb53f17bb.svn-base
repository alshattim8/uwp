#include "LList.h"

LList::~LList()
{
   if (list != NULL)
   {
      Node * p = list->next;
      while (p != NULL)
      {
         delete list;
         list = p;
         p = p->next;
      }
      delete list;
   }
}

bool LList::IsEmpty() const
{
   return list == NULL;
}

